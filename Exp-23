#include<iostream>
using namespace std;
void swap(int *x,int *y)
{
	int t;
	t=*x;
	*x=*y;
	*y=t;
}
int main()
{
	int i=10,j=20;
	cout<<"before swaping values:";
	cout<<i<<j;
	cout<< "after swaping: ";
	swap(&i, &j);
	cout<<i<<j;
}


#include<iostream>
using namespace std;
void swap(int x,int y)
{
	int t;
	t=x;
	x=y;
	y=t;
}
int main()
{
	int i=10,j=20;
	cout<<"before swaping values:";
	cout<<i<<j;
	cout<< "after swaping: ";
	swap(i,j);
	cout<<i<<j;
}
