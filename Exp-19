#include <iostream>
using namespace std;
class Complex {
private:
    double real;
    double imag;
public:
    Complex(double r = 0.0, double i = 0.0) : real(r), imag(i) {}
    Complex operator-(const Complex& other)
    {
        return Complex(real - other.real, imag - other.imag);
    }
    void display() const {
        cout << real;
        if (imag >= 0) {
            cout << " - " << imag << "i";
        } else {
            cout << " + " << +imag << "i";
        }
       cout << endl;
    }
};
int main() {
    Complex c1(4.0, 4.0);
    Complex c2(1.0, 2.0); 
    Complex c3 = c1 - c2;
    c3.display();
    return 0;
}
