#include <iostream>
using namespace std;
int main() {
    int size;
    int sum = 0;
    cout << "Enter the size of the array: ";
    cin >> size;
    int arr[100];
    cout << "Enter elements of the array:" << endl;
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
        sum += arr[i];  
    }
    cout << "The sum of the array elements is: " << sum << endl;
    return 0;
}
